@model List<Studenti>

<div class="container">

</div>

<table class="table table-bordered table-danger">
    <thead>
    <tr>
        <th>Id</th>
        <th>Emri</th>
        <th>Mbiemri</th>
    </tr>
    </thead>
    <tbody>
    @foreach (Studenti stud in Model)
    {
        <tr>
            <td>
                    @stud.Id
            </td>
            <td>
                @stud.Emri
            </td>
            <td>
                @stud.Mbiemri
            </td>
        </tr>
    }
   
    </tbody>
</table>